@model RemoteViewModel

<script>
const app =  
{
    remoteToken: "@Model.token",

    /*parameter serve for session initialization */
    SignallingUrl: "@Model.session.signallingurl",

    /**
    * RTP config, use to establish webrtc connection
    */
    RTPconfig:   
    {
        iceServers:
        [
            {
                urls:       "@Model.session.turnip",
                username:   "@Model.session.turnuser",
                credential: "@Model.session.turnpassword"
            },
            {
                urls: ["stun:stun.l.google.com:19302"]
            }
        ],
        "bundle-policy":"max-compat",
        "iceTransportPolicy" : "@Model.icePolicy"
    },
}

</script>