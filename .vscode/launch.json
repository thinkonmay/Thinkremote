    "version": "0.2.0",
    "configurations": [


        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/worker/port-forward/bin/Debug/net6.0/port-forward.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "(Windows) Attach",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "session core",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/session-core.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal"
        },
        {
            "name": "agent",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/agent.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal"
        },
        {
            "name": "remote app",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/remote-app.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal"
        }
    ]
}